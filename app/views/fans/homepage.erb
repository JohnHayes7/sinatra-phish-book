<h1>Welcome, <%= @fan.username %> </h1>
<h3>Here are your shows:</h3>
<% @fan.shows.each do |s| %>
<h3><%= s[:venue] %> </h3>
<h4><%= s[:location] %></h4>
<h4><%= s[:date] %></h4>
<p><%= s[:setlist] %></p>
    <% end %>
<div id="years_bar">
<h5>More Shows By Years:</h5>
<% Year.all.reverse.each do |y| %> 
    <ul>
        <li> <a href="/years/<%= y.value %>"> <%= y.value %> </a></li>
        <% end %>
    </ul> 

</div>

<form action='/fans/logout' method="post">
<button id="logput-btn" type="submit">LogOut</button>
</form>
